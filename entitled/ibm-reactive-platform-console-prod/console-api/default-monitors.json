        }
      },
      "prometheus_tsdb_reloads_failures": {
        "monitorVersion": "1",
        "model": "threshold",
        "parameters": {
          "metric": "prometheus_tsdb_reloads_failures_rate",
          "window": "5m",
          "confidence": "1",
          "severity": {
            "critical": {
              "comparator": ">=",
              "threshold": "1"
            }
