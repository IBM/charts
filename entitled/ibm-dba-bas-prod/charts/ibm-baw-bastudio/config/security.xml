<?xml version="1.0" encoding="UTF-8"?>
<server description="IBM BPM Process Server">
    <!-- DE Level Security Harden Configurations -->
    <variable name="XFrameOptionsHeaderValue" value="SAMEORIGIN" />
    <variable name="StrictTransportSecurityHeaderValue" value="" />
    <variable name="ContentSecurityPolicyHeaderValue" value="${env.CONTENT_SECURITY_POLICY}" />
    <variable name="XXssProtectionHeaderValue" value="1; mode=block" />
    <variable name="XContentTypeOptionsHeaderValue" value="nosniff" />
    <variable name="CsrfSessionTokenSalt" value="" />
    <variable name="CsrfSessionTokenProtectedUris" value="/teamworks/ajaxCoach" />
    <variable name="AllowedHttpMethods" value="GET,PUT,POST,DELETE,HEAD,OPTIONS" />
    <variable name="CsrfProtectionRefererWhitelist" value="${env.CSRF_REFERER_WHITELIST}" />
    <variable name="CsrfProtectionOriginWhitelist" value="" />
    <variable name="CommonLogoutExitPage" value="${ums_urlprefix}/oidc/endpoint/ums/logout" />
</server>

